parameters:

session:
	debugger: true
	expiration: 28 days

application:
	errorPresenter:
		4xx: Error:Error4xx
		5xx: Error:Error5xx
	mapping: App\Presentation\*\**Presenter

latte:
	strictParsing: yes
	extensions:
		- App\Presentation\Accessory\LatteExtension

assets:
	mapping:
		default:
			path: assets
			type: vite

di:
	export:
		parameters: no
		tags: no